{
  "name": "insane-focus-helper",
  "displayName": "subway-surfers-focus-helper",
  "description": "A simple extension to show a GIF in VSCode",
  "version": "0.0.3",
  "publisher": "alvseven",
  "repository": {
    "type": "git",
    "url": "https://github.com/alvseven/subway-surfers-vscode.git"
  },
  "engines": {
    "vscode": "^1.89.1"
  },
  "icon": "subway-surfers-icon.png",
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.showGif",
        "title": "Show Subway Surfers GIF"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "vsce:package": "vsce package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "~20.14.14",
    "@types/vscode": "^1.89.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@vscode/vsce": "^2.31.1"
  }
}
